                    <li><a href="#contacts" class="scroll-to">Contact</a></li>
                    <li><a href="@Url.Action("Index","Home")">Blog</a></li>
                </ul>
                <div class="divider"></div>
                @Html.Partial("_LoginPartial")
            </div><!-- Nav list -->
        </div>
