            <!-- feedback section -->
            <div class="row">
                <div class="col-xs-12 col-md-6 contacts-col-left">
                    <p class="text-right">Send me a message and I will connect back with you.</p>
                    <p class="text-right text-muted"><em>Dexter Norwood</em></p>
                </div>
                <div class="col-xs-12 col-md-6 contacts-col-right">
                    @using (Html.BeginForm("Contact","Home", FormMethod.Post, new { @class = "drop-line js-feedback-form"}))
                    {
                        @Html.AntiForgeryToken()
                            <span class="input">
                                <input class="input-field js-check-name" type="text" id="input-name" name="FromName">
                                <label class="input-label" for="input-name">
                                    <span class="input-label-content">Name</span>
                                </label>
                            </span>
                            <span class="input">
                                <input class="input-field js-check-email" type="email" id="input-mail" name="FromEmail">
                                <label class="input-label" for="input-mail">
                                    <span class="input-label-content">Your mail</span>
                                </label>
                            </span>
                            <span class="input">
                                <input class="input-field js-check-name" type="text" id="input-name" name="Subject">
                                <label class="input-label" for="input-name">
                                    <span class="input-label-content">Subject</span>
                                </label>
                            </span>
                            <span class="input">
                                <textarea class="input-field elastic js-check-message" rows="1" id="input-message" name="Body"></textarea>
                                <label class="input-label" for="input-message">
                                    <span class="input-label-content">Your message</span>
                                </label>
                            </span>
                            <button class="btn btn-default" type="submit">Sent Message</button>
                    }              
                </div>
            </div><!-- feedback section -->

        </div>
    </section>
